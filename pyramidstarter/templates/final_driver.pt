<div class="container-fluid" id="fullpage">

    <!--welcome box-->
    <div class="section"> <!--leftover from fullpage.js-->
        
        <div class="row" id="§driver">
        <div class="col-md-2"></div>
        <div class="col-md-8">

            <div class="page-header">
                <h1>Introduction</h1>
            </div>

        <div class="panel panel-default">
            <div class="panel-heading">
                <h1 class="panel-title">Driver</h1>
            </div>
            <div class="panel-body">
                <h2>Driver</h2>
                <h3>Description</h3>
                <p>bla bla</p>
                <h3>Results</h3>
                <form>
                <div class="row">

                        <div class="col-lg-3">
                            <div class="input-group">
                            <span class="input-group-addon" data-toggle="tooltip" title="Library size"
                                  data-placement="top">Library size</span>
                                <input type="number" min="0" class="form-control" placeholder="1600"
                                       id="driver_library_size">
                            </div>
                            <br/>
                        </div>
                        <div class="col-lg-3">
                            <div class="input-group">
                            <span class="input-group-addon" data-toggle="tooltip" title="Sequence length in nucleotides (or amino acids) N"
                                  data-placement="top">Length</span>
                                <input type="number" min="1" class="form-control" placeholder="1425"
                                       id="driver_length">
                            </div>
                            <br/>
                        </div>
                        <div class="col-lg-3">
                            <div class="input-group">
                            <span class="input-group-addon" data-toggle="tooltip" title="Mean number of crossovers per sequence m"
                                  data-placement="top">Mean</span>
                                <input type="number" min="0" class="form-control" placeholder="2"
                                       id="driver_mean">
                            </div>
                            <br/>
                        </div>
                </div>
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="input-group">
                                <span class="input-group-addon" data-toggle="tooltip" title="Pedel is basic and nucleotide only, Pedel-AA is more advanced and has amino acids" data-placement="top">Crossovers counting</span>
                                <input type="checkbox" class="switch" id="driver_crossover" checked data-off-text="Observable" data-on-text="All" data-off-color="warning" data-size="large" checked>
                            </div>
                            <br/>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-5">
                        <div class="input-group">
                    <span class="input-group-addon" data-toggle="tooltip" data-placement="top" title="Positions of variable nucleotides (or amino acids). (Maximum 15 or 20)">Positions
                        </span>
                    <textarea class="form-control custom-control" rows="2" style="resize:none" id="driver_positions" name="sequence"></textarea>
                </div>
                    <br/>
                        </div>

                </div><!-- row-->
                    </form>
                <div class="row">
                    <div class="col-lg-6 col-lg-offset-3">
                        <div class="btn-group" role="group" aria-label="...">
                            <button type="button" class="btn btn-warning" id="driver_clear"><i class="fa fa-eraser"
                                                                                             aria-hidden="true"></i>
                                Clear
                            </button>

                            <button type="button" class="btn btn-info" id="driver_demo"><i class="fa fa-files-o"
                                                                                         aria-hidden="true"></i> Demo
                            </button>
                            <button type="button" class="btn btn-success" id="driver_calculate"><i class="fa fa-exchange"
                                                                                                 aria-hidden="true"></i>
                                Calculate
                            </button>
                        </div>
                    </div>
                    <br/>
                </div>
                <!-- The results -->
                <div class="row hidden" id="glue_result">
                    <div class="alert alert-danger" role="alert"><span class="pycorpse"></span> Oh Snap. Something went
                        wrong
                    </div>
                    <br/>
                </div>
            </div>
        </div>
</div>
